<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AnimateGradientViewAttrs">
        <attr name="agv_angle" format="float" />
        <attr name="agv_startColor" format="color" />
        <attr name="agv_endColor" format="color" />
        <attr name="agv_middleColor" format="color" />
        <attr name="agv_startOffset" format="float" />
        <attr name="agv_middleOffset" format="float" />
        <attr name="agv_endOffset" format="float" />
        <attr name="agv_alpha" format="float" />
        <attr name="agv_gradient_scale" format="float" />
        <attr name="agv_autostart" format="boolean" />
        <attr name="agv_duration" format="integer" />
        <attr name="agv_animation" format="enum">
            <enum name="rotation" value="0" />
            <enum name="flow" value="1" />
        </attr>
        <attr name="agv_shape" format="enum">
            <enum name="stroke" value="0" />
            <enum name="water_drop" value="1" />
            <enum name="hole" value="2" />
            <enum name="notch" value="3" />
            <enum name="rectangle" value="4" />
        </attr>
        <attr name="agv_flow_direction">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
            <enum name="top" value="2" />
            <enum name="bottom" value="3" />
        </attr>
    </declare-styleable>
    <declare-styleable name="VerticalSeekBar">
        <attr name="vsb_click_to_set_progress" format="boolean" />
        <attr name="vsb_bar_corner_radius" format="dimension" />
        <attr name="vsb_bar_width" format="dimension" />
        <attr name="vsb_bar_background" format="reference|color" />
        <attr name="vsb_bar_background_gradient_start" format="color" />
        <attr name="vsb_bar_background_gradient_end" format="color" />
        <attr name="vsb_bar_progress" format="reference|color" />
        <attr name="vsb_bar_progress_gradient_start" format="color" />
        <attr name="vsb_bar_progress_gradient_end" format="color" />
        <attr name="vsb_progress" format="integer" />
        <attr name="vsb_max_value" format="integer" />
        <attr name="vsb_min_placeholder_position" format="enum">
            <enum name="outside" value="0"/>
            <enum name="inside" value="1"/>
            <enum name="middle" value="2"/>
        </attr>
        <attr name="vsb_min_placeholder_src" format="reference" />
        <attr name="vsb_max_placeholder_position" format="enum">
            <enum name="outside" value="0"/>
            <enum name="inside" value="1"/>
            <enum name="middle" value="2"/>
        </attr>
        <attr name="vsb_max_placeholder_src" format="reference" />
        <attr name="vsb_show_thumb" format="boolean" />
        <attr name="vsb_thumb_container_corner_radius" format="dimension" />
        <attr name="vsb_thumb_container_tint" format="color" />
        <attr name="vsb_thumb_placeholder_src" format="integer" />
        <attr name="vsb_use_thumb_to_set_progress" format="boolean" />
        <attr name="android:layout_width" />
        <attr name="android:layout_height" />

    </declare-styleable>
    <declare-styleable name="AudioWaveView">
        <attr format="color" name="waveColor"/>
        <attr format="color" name="waveFilledColor"/>
        <attr format="dimension" name="chunkWidth"/>
        <attr format="dimension" name="chunkHeight"/>
        <attr format="dimension" name="minChunkHeight"/>
        <attr format="dimension" name="chunkRadius"/>
        <attr format="dimension" name="chunkSpacing"/>
        <attr format="float" name="progress"/>
        <attr format="boolean" name="animateExpansion"/>
    </declare-styleable>
    <declare-styleable name="iiVisu">
        <attr name="loadedBarPrimeColor" format="color" />
        <attr name="backgroundBarPrimeColor" format="color" />
        <attr name="spaceBetweenBar" format="dimension" />
        <attr name="barWidth" format="dimension" />
        <attr name="approximateBarDuration" format="integer" />
        <attr name="maxAmp" format="float" />
    </declare-styleable>
</resources>