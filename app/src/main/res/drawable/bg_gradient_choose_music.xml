<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="5dp" />
            <gradient
                android:angle="135"
                android:endColor="#5DF1F7"
                android:startColor="#022E30" />
        </shape>
    </item>
    <item
        android:bottom="1dp"
        android:end="1dp"
        android:start="1dp"
        android:top="1dp">
        <shape android:shape="rectangle">
            <corners android:radius="5dp" />
            <solid android:color="#1A1C20" />
        </shape>
    </item>
</layer-list>