<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="5dp" />
            <gradient
                android:angle="135"
                android:endColor="#E33FF4"
                android:startColor="#3C0042" />
        </shape>
    </item>
    <item
        android:bottom="2dp"
        android:end="2dp"
        android:start="2dp"
        android:top="2dp">
        <shape android:shape="rectangle">
            <corners android:radius="5dp" />
            <solid android:color="#1A1C20" />
        </shape>
    </item>
</layer-list>